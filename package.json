  "scripts": {
    "clean": "bnr clean",
    "serve": "bnr serve",
    "analyze": "bnr analyze"
  },
  "betterScripts": {
    "clean": {
      "command": "rimraf dist"
    },
    "serve": {
      "command": "webpack-dev-server"
    },
    "analyze": {
      "command": "bnr clean && webpack --env.prod --env.analyze -p",
      "env": {
        "NODE_ENV": "production"
      }
    }
  },
  "dependencies": {
    "better-npm-run": "^0.0.15",
    "rimraf": "^2.6.1"
  },
  "devDependencies": {
    "@types/firebase": "^2.4.31",
    "@types/react": "^15.0.30",
    "@types/react-dom": "^15.5.0",
    "@types/redux": "^3.6.0",
    "@types/react-redux": "^4.4.45",
    "@types/redux-logger": "^3.0.0",
    "@types/redux-thunk": "^2.1.0",
    "babel-core": "^6.25.0",
    "babel-loader": "^7.1.0",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-env": "^1.5.2",
    "babel-preset-react": "^6.24.1",
    "css-loader": "^0.28.4",
    "extract-text-webpack-plugin": "^2.1.2",
    "firebase": "^4.1.3",
    "html-webpack-plugin": "^2.28.0",
    "node-sass": "^4.5.3",
    "normalize.css": "^7.0.0",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-redux": "^5.0.5",
    "redux": "^3.7.1",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.2.0",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.18.2",
    "ts-loader": "^2.2.0",
    "typescript": "^2.3.4",
    "webpack": "^3.0.0",
    "webpack-bundle-analyzer": "^2.8.2",
    "webpack-dev-server": "^2.5.0"
  }
}
